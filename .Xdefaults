!--------------------
!-[Xdefaults file]
!--------------------

!-[Font Rendering]-----
Xft.dpi:          99
Xft.lcdfilter:		lcdslight
Xft.antialias:		true
Xft.rgba:         rgb
Xft.hinting:		true
!Xft.hintstyle:    hintslight

!-[Fonts]----
!URxvt.font:xft:terminus:pixelsize=10:antialias=false
!URxvt.boldFont:xft:terminus:bold:pixelsize=10:antialias=false
!URxvt.font: xft:ProFontWindows:size=9:antialias=false
!URxvt.boldFont: xft:ProFontWindows:size=9:antialias=false
!URxvt.font:xft:ProggySmallTT:pixelsize=16
!URxvt.boldFont:xft:ProggySmallTT:pixelsize=16
!URxvt.font:xft:ProggyCleanTT:pixelsize=16
!URxvt.boldFont:xft:ProggyCleanTT:pixelsize=16
URxvt.font:xft:Inconsolata:hintstyle=hintslight:autohint=true:pixelsize=13
URxvt.boldFont:xft:Inconsolata:hintstyle=hintslight:autohint=true:pixelsize=13
!URxvt.font:xft:Envy Code R:hintstyle=hintfull:pixelsize=12
!URxvt.boldFont:xft:Envy Code R:hintstyle=hintfull:pixelsize=12
!URxvt.font:xft:DejaVu Sans Mono:pixelsize=10.5
!URxvt.boldFont:xft:DejaVu Sans Mono:pixelsize=10.5
!URxvt.font:xft:Tamsyn:pixelsize=14
!URxvt.boldFont:xft:Tamsyn:pixelsize=14

!-[URxvt Settings]----
URxvt.saveLines:	8192
URxvt.scrollBar:	false
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:	/usr/bin/jumanji
URxvt.matcher.button:	1
URxvt.underlineColor:   yellow
URxvt.geometry:		80x36
URxvt.transparent: 	false
URxvt.tintColor:	#000000
URxvt.shading:		60

!-[colors]----
URxvt.cursorColor: #DCDCCC

